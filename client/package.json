{
  "name": "dobro-client",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn typedCss && NODE_ENV=production gulp",
    "build:dev": "yarn clean:dist && yarn check:all && gulp dev",
    "build:bundle": "gulp devBundle",
    "check:all": "yarn csscomb && yarn prettier:fix && yarn lint && yarn typedCss && yarn checkTypes",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx  --fix",
    "prettier:ceck": "yarn prettier --check src",
    "prettier:fix": "yarn prettier --write src",
    "cache-clean": "rm -rf .fusebox/cache",
    "typedCss": "tcm -p 'src/**/*.scss'",
    "csscomb": "csscomb -c .csscomb.json ./src",
    "checkTypes": "tsc --noEmit",
    "clean:dist": "rm -rf dist",
    "generateIconType": "node ./build-scripts/generateIconType.js",
    "postinstall": "semantic-ui-css-patch",
    "test:u": "NODE_ENV=mock jest --detectOpenHandles .*.unit.ts",
    "test:c": "NODE_ENV=test jest --detectOpenHandles .*.spec.ts",
    "test": "yarn test:u && yarn test:c"
  },
  "devDependencies": {
