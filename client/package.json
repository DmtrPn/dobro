  "private": true,
  "scripts": {
    "wp": "webpack --config webpack.common.js",
    "es": "node esbuild.js",
    "build-dev": "vite",
    "build": "vite build",
    "build-dev:fuse": "yarn cache-clean && yarn tslint && yarn csscomb && yarn typedCss  && yarn checkTypes && node fuse dev",
    "build:fuse": "yarn cache-clean && NODE_ENV=production node fuse",
    "tslint": "tslint -c tslint.json src/**/*.{ts,tsx}",
    "tslint:fix": "tslint -c tslint.json src/**/*.{ts,tsx} --fix",
    "cache-clean": "rm -rf .fusebox/cache",
    "typedCss": "tcm -p 'src/**/*.scss'",
    "csscomb": "csscomb -c .csscomb.json ./src",
    "checkTypes": "tsc --noEmit",
    "generateIconType": "node ./scripts/generateIconType.js"
  },
  "devDependencies": {
