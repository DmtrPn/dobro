{
  "name": "dobro-server",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build-dev": "yarn clean-dist && yarn tslint && ./node_modules/.bin/tsc  --watch",
    "build": "yarn clean-dist && ./node_modules/.bin/tsc",
    "clean-dist": "rm -rf dist",
    "start-dev": "node-dev dist/app.js",
    "db:clear": "NODE_ENV=test node ./dist/bin/clear-db",
    "data:fix": "node dist/bin/dataFix.js",
    "start": "node dist/app.js",
    "tslint": "tslint -c tslint.json src/**/*.ts",
    "tslint:fix": "tslint --fix -c tslint.json src/**/*.ts",
    "migrate:test": "NODE_ENV=test yarn migrate",
    "migrate": "ts-node src/bin/db/migrations:run.ts",
    "migrate:undo": "ts-node src/bin/db/migrations:undo.ts",
    "test:unit": "NODE_ENV=test jest --detectOpenHandles .*.test.ts",
    "test:integration": "NODE_ENV=test jest --detectOpenHandles .*.spec.ts",
    "test": "yarn migrate:test && yarn test:unit && yarn test:integration",
    "generate:types": "node ./dist/bin/generate-api-types.js"
  },
  "dependencies": {
    "@nestjs/common": "8.4.1",
    "@nestjs/core": "8.4.1",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "7.1.3",
    "@nestjs/swagger": "5.2.1",
    "axios": "0.21.2",
    "body-parser": "1.18.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "config": "3.1.0",
    "connect-redis": "3.4.2",
    "dobro-types": "1.0.0",
    "dotenv": "16.0.0",
    "express": "4.17.1",
    "express-session": "1.16.1",
    "helmet": "3.9.0",
    "ioredis": "4.17.3",
    "lodash": "4.17.21",
    "log4js": "6.4.4",
    "passport": "0.5.2",
    "passport-local": "1.0.0",
    "password-crygen": "1.0.0",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7",
    "source-map-support": "0.5.21",
    "tsconfig-paths": "3.14.1",
    "typeorm": "0.3.4",
    "typescript-ioc": "3.2.2",
    "uuid": "8.3.2",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "@nestjs/testing": "8.4.3",
    "@types/chance": "1.1.3",
    "@types/connect-redis": "0.0.13",
    "@types/express": "4.17.2",
    "@types/express-session": "1.17.0",
    "@types/helmet": "0.0.37",
    "@types/jest": "27.4.1",
    "chance": "1.1.8",
    "dtsgenerator": "2.5.1",
    "jest": "27.5.1",
    "node-dev": "4.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "tslint": "5.17.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "4.5.5"
  }
}
